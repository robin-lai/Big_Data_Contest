#!/bin/bash

ROOT=/home/big_data_contest


THE_CONFIG=$ROOT/the_config


THE_CODE=$ROOT/the_code

CODE_STAT=$THE_CODE/stat

CODE_DATA=$THE_CODE/data


THE_DATA=$ROOT/the_data

DATA_IMAGE_SEARCH=$THE_DATA/image_search

DATA_IMAGE_SEARCH_CONTEST=$DATA_IMAGE_SEARCH/contest_data

DATA_IMAGE_SEARCH_TRAINING=$DATA_IMAGE_SEARCH/training_data

DATA_IMAGE_SEARCH_TEST=$DATA_IMAGE_SEARCH/test_data


THE_LOG=$ROOT/the_log

LOG_STAT=$THE_LOG/stat

LOG_GIT-PUSH=$THE_LOG/git-push

LOG_IMAGE_SEARCH=$THE_LOG/image_search
